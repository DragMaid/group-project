{
  "name": "@webapp",
  "scripts": {
    "dev": "pnpm next dev --turbopack",
    "build": "pnpm next build",
    "start": "pnpm next start",
    "lint": "pnpm next lint",
    "test": "pnpm jest --coverage",
    "deploy": "docker-compose up web --build --remove-orphans",
    "db:down": "pnpm --filter=@db db:down",
    "db:up": "pnpm dotenv-flow -e .env -- pnpm --filter=@db db:up",
    "db:build": "pnpm dotenv-flow -e .env -- pnpm --filter=@db db:build",
    "db:generate": "pnpm dotenv-flow -e .env -- pnpm --filter=@db generate",
    "db:migrate": "pnpm dotenv-flow -e .env -- pnpm --filter=@db migrate",
    "db:reset": "pnpm dotenv-flow -e .env -- pnpm --filter=@db format",
    "db:push": "pnpm dotenv-flow -e .env -- pnpm --filter=@db push",
    "db:studio": "pnpm dotenv-flow -e .env -- pnpm --filter=@db studio",
    "db:format": "pnpm dotenv-flow -e .env -- pnpm --filter=@db format",
    "db:seed": "pnpm --filter=@db seed"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "dotenv-flow-cli": "^1.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "next": "15.3.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint-config-next": "15.3.1"
  }
}
